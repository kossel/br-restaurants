{
  "presets": [
    ["env", {
      "targets": {
        "chrome": 52,
        "browsers": ["last 2 versions", "safari 7"]
      },
      "modules": false,
      "loose": true
    }],
    "react"
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "test": {
      "presets": ["env", "react"]
    }
  },
  "plugins": [
    "transform-object-rest-spread",
    ["transform-runtime", {
      "polyfill": false
    }]
  ]
}
